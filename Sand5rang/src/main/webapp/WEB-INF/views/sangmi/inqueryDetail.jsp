                 function postFormSubmit(num){
                     if(num == 1){ //수정 요청으로 action 속성값 바꾸기
	            		  
	            		  $("#postForm").attr("action","inUpdate.sm").submit();
	            	  }
	            	  else{ // 삭제 요청으로 action 속성값 바꾸기
